[STARTUP]
FF_ROM=fx2_aforce
BACKGLASS_EXISTS=YES
COLOUR_PALETTE=Red,Lime,Gold,Slate_Blue,Tomato,Silver,Dark_Orchid,Spring_Green
BUTTON_COLOUR_CHANGE=BUT_LF,Gold,BUT_RF,Gold,BUT_ML,Tomato,BUT_MR,Tomato

[COMMANDS]
GAMEMODE|ON|FF_Colour Black,RGB_DF,0|FF_Flasher DV_FLOL,FL_FD,6,750,50,Random|FF_Flasher DV_FLOR,FL_FD,6,750,50,Random|FF_Flasher DV_FLIL,FL_FD,6,750,50,Random|FF_Flasher DV_FLIR,FL_FD,6,750,50,Random|FF_Flasher DV_FLCN,FL_FD,6,750,50,Random|FF_DOF E424,-1
LeftFlipper|On|FF_Dev DV_LF,10000|FF_DOF E101,-1
LeftFlipper|OFF|FF_Dev DV_LF,0|FF_DOF E101,0
RightFlipper|On|FF_Dev DV_RF,10000|FF_DOF E102,-1
RightFlipper|OFF|FF_Dev DV_RF,0|FF_DOF E102,0
MENUMODE|On|FF_Flasher DV_FLOL,FL_OFF,1,1,100,Random|FF_Flasher DV_FLIL,FL_OFF,1,1,100,Random|FF_Flasher DV_FLCN,FL_OFF,1,1,100,Random|FF_Flasher DV_FLIR,FL_OFF,1,1,100,Random|FF_Flasher DV_FLOR,FL_OFF,1,1,100,Random|FF_Dev DV_SR,0|FF_Dev DV_BK,0|FF_B2S B2SStopAllAnimations
LAUNCHMODE|On|FF_Button BUT_LB,BA_FL,10000,500|FF_DOF E310,-1
LAUNCHMODE|OFF|FF_Button BUT_LB,BA_OFF,0,0|FF_DOF E310,0
NudgeLeft|On|FF_Dev DV_SH,4000|FF_Colour Red,RGB_TT,1500|FF_Flasher DV_FLOL,FL_ON,1,1,100,Red|FF_Flasher DV_FLIL,FL_ON,1,1,100,Red|FF_DOF E403,-1
NudgeLeft|OFF|FF_Dev DV_SH,250|FF_Flasher DV_FLOL,FL_OFF,1,1,100,Red|FF_Flasher DV_FLIL,FL_OFF,1,1,100,Red
NudgeRight|On|FF_Dev DV_SH,4000|FF_Colour Red,RGB_TT,1500|FF_Flasher DV_FLOR,FL_ON,1,1,100,Red|FF_Flasher DV_FLIR,FL_ON,1,1,100,Red|FF_DOF E404,-1
NudgeRight|OFF|FF_Dev DV_SH,250|FF_Flasher DV_FLOR,FL_OFF,1,1,100,Red|FF_Flasher DV_FLIR,FL_OFF,1,1,100,Red
NudgeForward|On|FF_Dev DV_SH,4000|FF_Colour Red,RGB_TT,1500|FF_Flasher DV_FLCN,FL_ON,1,1,100,Red|FF_DOF E405,-1
NudgeForward|OFF|FF_Dev DV_SH,250|FF_Flasher DV_FLCN,FL_OFF,1,1,100,Red
LAUNCHBALL|OFF|FF_Dev DV_MR,-1|FF_DOF E132,-1

