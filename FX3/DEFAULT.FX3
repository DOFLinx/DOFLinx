[STARTUP]
FF_ROM=fx2_aliens
BACKGLASS_EXISTS=NO
BUTTON_COLOUR_CHANGE=BUT_LF,Random,BUT_RF,Random,BUT_ML,Orange,BUT_MR,Orange

[COMMANDS]
LeftFlipper|ON|FF_Dev DV_LF,10000|FF_DOF E101,-1
