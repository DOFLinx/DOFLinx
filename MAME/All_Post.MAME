[STARTUP]
BUTTONS_LIT_ADDED=BUT_ST,BUT_CN,BUT_EX,BUT_RE
KEY_TO_COMMAND_ADDED=BUT_CN,coin,BUT_P1,player1,BUT_P2,Player2

[SHUTDOWN]
